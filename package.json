{
  "name": "post-command-hook",
  "version": "2.0.1",
  "description": "Run a command and then all registered hook(s)",
  "homepage": "https://github.com/akgondber/post-command-hook",
  "author": {
    "name": "Rushan Alyautdinov",
    "email": "akgondber@gmail.com",
    "url": "https://github.com/akgondber"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "keywords": [
    "command",
    "hook",
    "productivity",
    "optimization",
    "workflow",
    "cli",
    "shell",
    "terminal",
    "tool"
  ],
  "devDependencies": {
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-xo": "^0.39.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^12.1.2",
    "prettier": "^2.4.1"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "lint": "eslint . --fix",
    "test": "jest"
  },
  "repository": "akgondber/post-command-hook",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT",
  "dependencies": {
    "conf": "^10.1.0",
    "consola": "^2.15.3",
    "execa": "5.1",
    "is-plain-object": "^5.0.0",
    "lodash.isnil": "^4.0.0",
    "lodash.isstring": "^4.0.1",
    "object-hash": "^2.2.0"
  }
}
